{
	"name": "temp-project",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"start": "wrangler d1 migrations apply satomatashikitask-db --local --batch-size 100 && npm run build && wrangler pages dev .svelte-kit/cloudflare --persist-to=.wrangler/state",
		"build": "vite build",
		"preview": "vite preview",
		"db:check": "node check-migrations.cjs",
		"db:reset": "node -e \"const fs=require('fs');const p='.wrangler/state/v3/d1';if(fs.existsSync(p))fs.rmSync(p,{recursive:true,force:true})\" && wrangler d1 migrations apply satomatashikitask-db --local",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/bcryptjs": "^2.4.6",
		"@types/better-sqlite3": "^7.6.13",
		"autoprefixer": "^10.4.22",
		"better-sqlite3": "^12.4.1",
		"postcss": "^8.5.6",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.1.10",
		"wrangler": "^4.49.0"
	},
	"dependencies": {
		"bcryptjs": "^3.0.3",
		"chart.js": "^4.5.1",
		"lucide-svelte": "^0.554.0",
		"svelte-dnd-action": "^0.9.67"
	}
}
